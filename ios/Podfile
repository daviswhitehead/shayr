platform :ios, '11.2'

target 'shayr' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  # Pods for shayr
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'

  target 'ShareExtension' do
    inherit! :complete
  end

  target 'shayr-tvOSTests' do
    inherit! :search_paths
  end

  target 'shayrTests' do
    inherit! :search_paths
  end

end
