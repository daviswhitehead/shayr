{
  "name": "shayr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "dsyms": "find ./ios/ -name \"*.dSYM\" | xargs -I {} /Applications/Fabric.app/Contents/MacOS/upload-symbols -a <api-key> -p ios {}",
    "release:ios:beta": "bundle exec fastlane ios beta",
    "release:android:beta": "bundle exec fastlane android beta",
    "debugger": "open \"rndebugger://set-debugger-loc?host=localhost&port=8081\"",
    "ios:dev": "sh scripts/env_manager.sh dev && ENVFILE=./environments/dev/.env.dev react-native run-ios --scheme shayr-dev",
    "ios:prod": "sh scripts/env_manager.sh prod && ENVFILE=./environments/prod/.env.prod react-native run-ios --scheme shayr-prod",
    "ios:staging": "sh scripts/env_manager.sh staging && ENVFILE=./environments/staging/.env.staging react-native run-ios --scheme shayr-prod",
    "reset_node_modules": "rm -rf node_modules/ && npm cache verify && npm install"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "fb": "^2.0.0",
    "fbjs": "^0.8.16",
    "lodash": "^4.17.5",
    "react": "^16.5.0",
    "react-native": "^0.57.0",
    "react-native-action-button": "^2.8.4",
    "react-native-config": "^0.11.5",
    "react-native-fbsdk": "^0.7.0",
    "react-native-firebase": "^5.0.0",
    "react-native-root-toast": "^3.0.1",
    "react-native-share-extension": "^1.2.1",
    "react-native-swipeable": "^0.6.0",
    "react-native-swiss-knife": "0.0.5",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^2.2.5",
    "react-navigation-redux-helpers": "^1.1.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-jest": "22.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.5.0",
    "jest": "22.1.4",
    "metro-react-native-babel-preset": "^0.49.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./app/assets/fonts/"
    ]
  }
}
